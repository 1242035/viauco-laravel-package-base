{
    "name": "viauco/base",
    "description": "Viauco laravel base package",
    "keywords":    [
        "laravel", "Viauco"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php":               ">=7.1.3",
        "laravel/framework": "*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0",
        "mockery/mockery":     "~1.1",
        "phpunit/phpunit":     "~7.0|~8.0",
        "phpunit/phpcov":      "~5.0|~6.0"
    },
    "autoload": {
        "psr-4": {
            "Viauco\\Base\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viauco\\Base\\Tests\\": "tests/"
        }
    }
}
